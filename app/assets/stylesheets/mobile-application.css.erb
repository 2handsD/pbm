#lookup_map, #container {
  margin: 0 auto;
  width: auto;
  min-width: 320px;
}
#header {
  position: relative;
  box-shadow: none;
  width: auto;
  min-width: 320px;
  height: auto;
  text-align: center;
  background-color: #f6effa;
  margin: 0 auto;
}
#header a img {
  float: none;
  padding: 0;
  width: 100%;
  /*margin-bottom: -10px;*/
  max-width: 460px;
}
#header ul {
  display: inline-block;
  margin: 0 auto;
  padding-bottom: 10px;
  float: none;
  width: 100%;
  min-width: 320px;
  font-size: 16px;
  background-color: #f6effa;
  text-align: center;
}

.other_regions {
    margin: 0 auto;
    width: 100%;
    font-size: 20px;
}

.new_blog_region, .store_buy {
  display: block;
  float: none;
  font-size: 18px;
}

#header span.region_name_page a {
  font-size: 30px;
}

#header.header_home {
  min-width: 320px;
  padding: 0;
}

#header .region_logo_dropdown {
    float: none;
    background: #f6effa;
    width: auto;
}

.pick_a_map span.pick_a_map {
  display: none;
}

div.pick_a_map {
  padding-top: 0;
}

.new_blog {
  width: 100%;
  padding: 12px 0 12px;
  text-align: center;
  border-bottom: 2px solid #fdfcfe;
  font-size: 18px;
}

.map_summary {
  margin-right: 5px;
}

#welcome {
  width: auto;
  min-width: 320px;
  margin-top: 10px;
}

#map {
  position: relative;
  width: auto;
  min-width: 300px;
  height: 300px;
  right: 0;
  margin: 0 10px 10px;
  top: 0;
  z-index: 500;
  border: none;
}

#map_summaries {
  margin: 0 0 0 10px;
}

#locations {
  margin: 0 10px;
}
#locations p {
  margin-top: 0;
}

.home_intro {
  margin: 0 10px 10px 10px;
  width: auto;
}

.home_intro_container {
  float: none;
}

#intro {
  padding: 10px;
  max-width: 100%;
}

#intro p, #intro .dark, #intro .darkb {
  font-size: 18px;
  line-height: 22px;
}

.new_blog {
  border-right: none;
}

.scrollerWrapper, #lookup img, #lookup  {
  display: none;
}

.section, .location_name, .location_detail_location ul, .location_detail {
  box-shadow: none;
  border-radius: 0;
}

.section {
  width: auto;
  height: auto;
  border: none;
  padding: 5px 8px;
  margin-top: 0;
}

#form {
  width: auto;
  min-width: 300px;
  height: auto;
  top: 0;
  right: 0;
  position: relative;
  margin: 10px;
  background-color: none;
  z-index: 0;
  padding-bottom: 0;
  padding-top: 0;
}

#form .search {
  float: none;
  display: block;
  width: 100%;
  font-size: 20px;
  height: 30px;
  margin: 10px 0;
}

#form select {
  margin-right: 5px;
}

#form input.lookup_search_input {
    margin-left: 0;
    font-size: 18px;
    height: 28px;
    width: 100%;
}

.location_search {
  margin-left: 0;
}

.location_search span {
    font-size: 16px;
}

#other_search_options {
  text-align: center;
  padding: 0 0 4px 0;
  width: auto;
  float: none;
}

#by_type_id, #by_operator_id, #by_location_id, #by_machine_id, #by_zone_id, #by_city_id {
  font-size: 18px;
  width: 100%;
  margin: 10px 0;
}

.location_link {
  font-size: 16px;
  height: 18px;
  line-height: 18px;
  padding: 6px 0;
  border: 1px solid #494949;
  border-left: none;
  border-right: none;
  margin: 0;
}

.search_result ul {
  border: none;
  border-radius: 0;
}

.search_result .show_location_detail_location ul {
  margin-top: 0;
}

.name {
  font-size: 20px;
  line-height: 24px;
  border-radius: 0;
}

.address, .address_name {
  font-size: 16px;
  line-height: 20px;
}

.machine_name {
  width: auto;
  font-size: 20px;
  line-height: 28px;
  float: none;
}

.machine_names {
  font-size: 18px;
  line-height: 24px;
}

.location_detail input.remove_machine_button {
  font-size: 16px;
  height: 28px;
  width: 100px;
}

.search_result {
  border-radius: 0;
  padding: 0;
  border: 2px solid #987673;
}

.location_detail {
  padding: 0;
  font-size: 14px;
  border: none;
  width: 98%;
}

.desc_location {
  font-size: 14px;
  line-height: 18px;
}

.machine_condition_display_lmx {
  font-size: 15px;
  line-height: 20px;
}

.sub_nav_item {
  height: auto;
  padding: 10px 0;
}

.sub_nav_item span {
  display: block;
  padding-left: 10px;
  font-weight: bold;
}

.sub_nav_item, .sub_nav_location {
  font-size: 20px;
  line-height: 24px;
}

#results_container, .search_result, .location_detail,
.location_name, .sub_nav_item, .sub_nav_location {
  width: 100%;
}

#search_results {
  margin: -5px 14px 10px 10px;
  width: auto;
}

.location_name {
  margin: -14px 0 0;
}

.high_score > .sub_nav_item, .score_container_lmx > .sub_nav_item {
  min-width: 240px;
  width: auto;
}

.high_score span, .score_container_lmx span {
    font-size: 16px;
    line-height: 16px;
}

.desc_edit_location .edit_mode, .machine_condition_display_lmx .edit_mode {
  max-width: 236px;
  min-width: 236px;
  width: auto;
}

.add_picture_location span.info, .show_machines_location span.info, 
.add_machine_location span.info, .add_picture_location, .new_location_picture_xref, 
.add_machine_location, .show_machines_location {
  width: auto;
  margin: 0;
  padding: 10px;
  font-size: 14px;
  line-height: 18px;
}

.search, .submit_button, .save_button, .cancel_button, #add_score, #add, 
#location_picture_xref_submit, .new_location_picture_xref input.submit_picture, input#add {
  width: 100%;
  font-size: 16px;
  height: 30px;
  padding: 0;
  margin: 10px 0;
}

.submit_button {
  width: 98%;
}

.thanks, p.alert {
  margin: 0 auto;
}

#file_chooser_button {
  width: auto;
}

.add_scores_form, .show_scores_lmx {
  width: auto;
  font-size: 16px;
}

#add_machine_by_id, #add_machine_by_name {
  width: 100%;
  display: block;
  font-size: 16px;
  height: 28px;
  margin: 0;
}

.machine_condition_display_lmx, .score_result .sub_nav_item, .score_result {
  width: auto;
}

.score_result span {
  float: left;
  padding-bottom: 0;
}

input#score, input#initials {
    width: 170px;
    font-size: 16px;
  line-height: 24px;
}

#other_search_options a {
  line-height: 36px;
}

#other_search_options span {
    display: block;
    font-size: 18px;
    padding-top: 5px;
}

#operator_section_link, #zone_section_link, #machine_section_link, 
#city_section_link, #location_section_link, #type_section_link {
  background-color: #E9E7E9;
  font-size: 20px;
}

#lookup {
  box-shadow: none;
  display: block;
  float: none;
  width: 100%;
  min-width: 320px;
  position: absolute;
  margin-top: 10px;
  padding: 0;
  margin-right: 0;
  border: none;
  background-color: transparent;
}

.links_column, #left_column, .left_column, .right_column_outer
.left_column.suggest, .left_column.high, .left_column.secondary,
.right_column, #right_column, .left_column_outer, .right_column.secondary,
.score_result {
  margin: 10px !important;
}

.left_column_about {
  margin-left: 0;
}

#page_body {
  width: 100%;
  min-width: 0;
}

#left_column, .left_column, .left_column_about, 
#region_page_body, .links_column, .right_column_outer .links_column, .links_category, .left_column_outer, .right_column_outer,
.left_column.suggest, .left_column.high, .left_column.secondary,
.right_column, #right_column, .right_column.right_column_apps, .right_column.secondary {
  width: auto;
  min-width: 0;
  min-height: 0;
}

.right_column, #right_column, .left_column.secondary,
#left_column, .left_column, .left_column_about {
  float: none;
  display: block;
  clear: both;
}

.right_column.secondary .ios img,
.right_column.secondary .ipad img,
.right_column img.store_shirt {
  width: 98%;
  height: auto;
}

#location_maker li {
  clear: both;
  margin-right: 0;
}

#location_maker ul, #contact_maker ul {
  padding-left: 0;
}

#contact_maker textarea {
  width: 98%;
  max-width: 98%;
}

#contact_maker textarea {
  min-height: 100px;
}

#location_maker ul {
  width: 100%;
}

#location_maker li, #contact_maker li {
  margin-bottom: 10px;
  height: 50px;
}

#location_maker input.text, #contact_maker input.text {
  font-size: 16px;
  height: 28px;
  width: 98%;
}

#location_maker li.submit, #contact_maker li.submit {
  margin-bottom: 30px;
}

#location_maker .first {
  float: none;
}

#location_maker textarea.machines {
  width: 98%;
  min-height: 100px;
}

#login {
  margin: 10px auto 0;
}

p.notice {
  margin: 10px auto 10px;
}

@media (max-width: 915px) { 

#form {
  left: 0;
}

#lookup, .scrollerWrapper {
  left: 0;
  float: none;
}
}

@media (max-width: 973px) {

  .home_intro_right {
    left: 0;
  }

  #welcome {
    width: auto;
  }

}

@media (max-width: 320px) {

  .map_summary {
    width: 137px;
  }

}
